[core]
	precomposeunicode = true
	excludesfile = ~/.gitignore
	quotepath = false
[github]
	user = hulkoba
[credential]
	helper = cache --timeout=3600
[push]
	default = simple
	followTags = true
[alias]
	a = add .
	rb = rebase --continue
	st = status
	ci = commit -am
	co = checkout
	cob = checkout -b
	df = diff
	pr = pull -r origin master
	please = --force-with-lease
[color]
	status = always
	ui = true
[color "status"]
  added = yellow
  changed = green
	untracked = magenta


